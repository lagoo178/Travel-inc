<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.trvl.travelinc">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".Travelinc"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".user"></activity>
        <activity android:name=".status" />
        <activity android:name=".setting" />
        <activity android:name=".profil" />

        <service
            android:name=".firebasemessagingservice"
            android:enabled="true"
            android:exported="true" />

        <activity
            android:name=".chat"
            android:screenOrientation="portrait" />
        <activity
            android:name=".login"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".nongkrong_detail"
            android:screenOrientation="portrait" />
        <activity
            android:name=".wisata_bali"
            android:screenOrientation="portrait" />
        <activity
            android:name=".nongkrong_bali"
            android:screenOrientation="portrait" />
        <activity
            android:name=".nongkrong_jogja"
            android:screenOrientation="portrait" />
        <activity
            android:name=".nongkrong_jakarta"
            android:screenOrientation="portrait" />
        <activity
            android:name=".chatlist"
            android:screenOrientation="portrait" />
        <activity
            android:name=".kategori"
            android:screenOrientation="portrait" />
        <activity
            android:name=".home"
            android:screenOrientation="portrait" />
        <activity
            android:name=".register"
            android:screenOrientation="portrait" />
    </application>

</manifest>